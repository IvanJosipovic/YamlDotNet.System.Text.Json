{
  "branches": [
    "main",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "semantic-release/changelog",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "CHANGELOG.md"
          },
          {
            "path": "src/YamlDotNet.System.Text.Json/bin/Release/*.nupkg"
          }
        ]
      }
    ]
  ]
}